#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int diff  = n;
    int arr[n];
    int count = 0;
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    int tr;
    cin>>tr;
   
   for(int i=0;i<n;i++)
 {
     int div=0;
     div=arr[i]/tr;
     if(arr[i]%tr!=0)
         div++;
         cout<<div<<endl;
     count+=div;
 }
 cout<<count;
        
    
    return 0;
}
