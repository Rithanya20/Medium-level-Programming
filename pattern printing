#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int diff  = n;
    int c = n;
    
    for(int i=0; i<n; i++){
        int dis = 1+i;
        for(int j=-1; j<c-1; j++){
            if(j<0){
            cout<<dis<<" ";
            }
            else{
            dis = dis+n-j;
            cout<<dis<<" ";
            }
            
            
        }
        c = c-1;
        cout<<endl;
    }
    
    return 0;
}
