 #include<bits/stdc++.h>
using namespace std;
int main(){
  int r;
  cin>>r;
  int space = r*2;
  int bar = r-1;
  for(int i=0; i<r; i++){

    for(int j=0; j<space-1; j++){
        if(j < bar){
            cout<<" ";
        }
        else{
            cout<<"*";
        }

    }
    space--;
    bar--;
    cout<<endl;
  }
return 0;
}
