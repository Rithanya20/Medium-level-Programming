#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int a[1000];
    a[0] = 3, a[1]=4;
    int k=1;
    for(int i=0; i<n; i++){
        a[++k] = (a[i]*10) + 3;
        a[++k] = (a[i]*10) + 4;
    }
    for(int i=0; i<n; i++){
        cout<<a[i]<<" ";
    }
    
    return 0;
}
