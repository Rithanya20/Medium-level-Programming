#include<bits/stdc++.h>
using namespace std;
int main(){
    string t;
    cin>>t;
    int n =t.size();
    for(int i=0; i<t.size();i++){
        for(int j=0; j<t.size(); j++){
            if(i==j){
                cout<<t[i];
                if(i == t.size()/2){
                   --n;
            }
            }
            else if(i+j==t.size()-1){
                cout<<t[--n];
               
            }
            else{
                cout<<" ";
            }
            
        }
        cout<<endl;
    }
   return 0;
}
