#include<bits/stdc++.h>
using namespace std;
int main(){
    int r;
    cin>>r;
    int c = (r*2)-1;
    //cout<<c;
    for(int i=0; i<r; i++){
        for(int j=0;j<i; j++ )
        {
            cout<<" ";
        }
        for(int j=0; j<c; j++){
            cout<<"*";
            
        }
        c = c-2;
        cout<<endl;
    }
    
    
    return 0;
}
